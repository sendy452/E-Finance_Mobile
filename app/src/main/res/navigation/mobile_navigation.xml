<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.google.e_finance.ui.home.HomeFragment"
        android:label="Dashboard"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_pendapatan"
        android:name="com.google.e_finance.ui.pendapatan.PendapatanFragment"
        android:label="Pendapatan"
        tools:layout="@layout/fragment_pendapatan" />

    <fragment
        android:id="@+id/nav_insert_pendapatan"
        android:name="com.google.e_finance.ui.pendapatan.UpdateData"
        android:label="Pendapatan"
        tools:layout="@layout/activity_insert" />

    <fragment
        android:id="@+id/nav_pengeluaran"
        android:name="com.google.e_finance.ui.pengeluaran.PengeluaranFragment"
        android:label="Pengeluaran"
        tools:layout="@layout/fragment_pengeluaran" />

    <fragment
        android:id="@+id/nav_laporan"
        android:name="com.google.e_finance.ui.laporan.LaporanFragment"
        android:label="Laporan"
        tools:layout="@layout/fragment_laporan" />

    <fragment
        android:id="@+id/nav_setting"
        android:name="com.google.e_finance.ui.setting.SettingFragment"
        android:label="Setting"
        tools:layout="@layout/fragment_setting" />
</navigation>